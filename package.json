{
  "name": "4q",
  "version": "0.6.0",
  "description": "command-line 4q archive tools",
  "keywords": [
    "archive"
  ],
  "author": "Robey Pointer <robeypointer@gmail.com>",
  "homepage": "https://github.com/robey/4q",
  "repository": {
    "type": "git",
    "url": "git://github.com/robey/4q.git"
  },
  "licenses": [
    {
      "type": "Apache License 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.txt"
    }
  ],
  "scripts": {
    "b": "babel --source-maps --out-dir lib src && babel --source-maps --out-dir test/lib test/src",
    "clean": "rm -rf lib test/lib",
    "distclean": "npm run clean && rm -rf node_modules",
    "prepublish": "babel --source-maps --out-dir lib src && babel --source-maps --out-dir test/lib test/src",
    "test": "mocha -R spec --colors test/lib --timeout 60000 --slow 1000"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "bluebird": "*",
    "clicolor": "*",
    "errno": "*",
    "lib4q": "*",
    "minimist": "*",
    "sprintf": "*",
    "stream-toolkit": "^2.0.0",
    "strftime": "*"
  },
  "devDependencies": {
    "babel": "*",
    "mocha": "*",
    "mocha-sprinkles": "*",
    "should": "*",
    "source-map-support": "*",
    "json5": "*"
  },
  "main": "./lib/4q",
  "files": [
    "bin",
    "build.plz",
    "docs",
    "lib",
    "test"
  ]
}